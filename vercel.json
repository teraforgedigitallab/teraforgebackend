{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://teraforgedigitallab.com",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS"],
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "sandbox"
  }
}
